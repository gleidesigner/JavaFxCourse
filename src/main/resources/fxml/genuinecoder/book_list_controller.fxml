<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPaneListBook" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass"
            stylesheets="@../../styles/genuinecoder/book_list.css" xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gleideveloper.com.br.genuinecoder.ui.book.BookListController">
    <children>
        <TableView fx:id="tableViewBookList" layoutX="29.0" layoutY="55.0" prefHeight="400.0" prefWidth="600.0"
                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="idCol" prefWidth="75.0" text="BookID"/>
                <TableColumn fx:id="titleCol" prefWidth="75.0" text="Title"/>
                <TableColumn fx:id="authorCol" prefWidth="75.0" text="Author"/>
                <TableColumn fx:id="publisherCol" prefWidth="75.0" text="Publisher"/>
                <TableColumn fx:id="availabCol" prefWidth="75.0" text="Availability"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
